<data-extraction-rules>
    <cloud-backup>
        <!-- Include user preferences and important data files -->
        <include domain="file" path="shared_prefs/my_app_preferences.xml" />
        <include domain="database" path="my_app_database.db" />

        <!-- Exclude cache files and other non-essential data -->
        <exclude domain="file" path="cache/" />
        <exclude domain="file" path="files/temp_data/" />
    </cloud-backup>
</data-extraction-rules>